<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".NoEventsFragment"
        android:id="@+id/frameLayout2"
        tools:layout_editor_absoluteY="81dp">

    <TextView
            android:text="@string/no_events_fragment_message"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:id="@+id/no_events_message1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="19dp"
            app:layout_constraintBottom_toTopOf="@+id/no_events_message2"
            android:layout_marginTop="107dp"
            app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:text="@string/no_events_fragment_message2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/no_events_message2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/no_events_message1"/>

</android.support.constraint.ConstraintLayout>